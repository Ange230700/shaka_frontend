{
  "name": "shaka_frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "lint": "eslint . --cache --fix",
    "copy-leaflet": "cp -r node_modules/leaflet/dist/images/ public/images/",
    "clean": "rm -rf public/images/* package-lock.json node_modules .expo dist",
    "postinstall": "npm run copy-leaflet",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:ci": "jest --runInBand --detectOpenHandles",
    "export": "expo export"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.11.0",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.13",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "leaflet": "^1.9.4",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-leaflet": "^5.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-native-worklets": "^0.4.1",
    "react-redux": "^9.2.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@faker-js/faker": "^10.0.0",
    "@react-native/eslint-config": "^0.81.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "lint-staged": "^16.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3",
    "validate-branch-name": "^1.3.2"
  },
  "overrides": {
    "react-test-renderer": "19.0.0"
  },
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js, jsx, ts, tsx}": "eslint . --cache --fix"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|hotfix|release)/.+$",
    "errorMsg": "Branch name must be master, main, develop, feature/xxx, fix/xxx, hotfix/xxx or release/xxx"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=20"
  }
}
